include "stdtest.ph";

class foo
{
    var x:std.int;
    var y:std.int;
}

class bar
{
    var a:foo, b:foo;
}

func main()
{
    var a:bar();
    a.a.x = 123;
    a.b.x = 123;

    a.a.y = 321;
    a.b.y = 321;

    var b:bar(), c:bar();

    b = c = a;

    test.assert(b.a.x == 123);
    test.assert(b.b.y == 321);

    test.assert(c.a.x == 123);
    test.assert(c.b.y == 321);
}
