include("stdtest.ph");

class foo
{
    func new(n:std.int) ... x(n) { }

    var x:std.int;
}

class bar
{
    func new() ... x(123), y(321) { }

    var x:foo, y:foo;
}

class woo
{
    var x:bar;
}

func main()
{
    var x = woo();

    test.assert(x.x.x.x == 123);
    test.assert(x.x.y.x == 321);
}
